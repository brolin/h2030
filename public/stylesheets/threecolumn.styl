header
  height: 5%
  float: left
  width: 100%
  background-color: #666
  color: white
  position: absolute
  top: 0

.timelines-list
  position: relative
  top: 5%
  width: 100%
  overflow: hidden
  height: 94%
  ul
    margin: 0
    padding: 0
    height: 30%
  .timeline-item
    display: block
    border: 1px solid #AAA
    padding: 15px
    width: 15000px
    p.name
      font-size: 12px
      padding: 3px 10px
      corner(4px)
      color: white
    div.event
      box-shadow: 0px 0px 6px #505050;
      display: inline-table
      corner(4px)
      margin: 3px
      padding: 5px
      background-color: PaleTurquoise
      

.timelines-control
  position: absolute
  bottom: 0px
  width: 100%
  height: 3%
  background-color: #666
  ul
    float: left
    color: white
    width: 100%
  li
    float: left
    padding-left: 10px
    padding-right: 10px

.span-control
  position: absolute
  height: 98%
  width: 20%
  border: 2px solid black
  background: transparent
  